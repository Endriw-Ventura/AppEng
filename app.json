{
  "expo": {
    "name": "Helpet",
    "slug": "Helpet",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": ["@react-native-google-signin/google-signin"],
    "icon": "./assets/images/background/helpet bg.jpeg",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/background/helpet bg.jpeg",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/background/helpet bg.jpeg"
    }
  }
}
